<?xml version="1.0" ?>
<odoo noupdate="1">
    <record id="payment_provider_btcpay" model="payment.provider">
        <field name="name">btcpay</field>
        <field name="code">btcpay</field>
        <field name="is_crypto_provider">true</field>
        <field name="company_id" ref="base.main_company"/>
        <field name="image_128" type="base64" file="mlr_ecommerce_btcpay_2/static/description/icon.png"/>
        <field name="inline_form_view_id" ref="btcpay_inline_form"/>
        <field name="redirect_form_view_id" ref="redirect_form"/>
        <field name="payment_method_ids"
               eval="[Command.set([
                        ref('mlr_ecommerce_btcpay_2.payment_method_btcpay'),
                    ])]"
        />
    </record>
</odoo>
